<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="288.0" prefWidth="378.0" style="-fx-background-color: #FFFF;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.eprsystem.SignupController">
   <children>
      <TextField fx:id="nameField" layoutX="114.0" layoutY="33.0" promptText="Name:" style="-fx-border-color: rgb(186, 228, 179); -fx-border-radius: 5;" />
      <TextField fx:id="cnpjField" layoutX="114.0" layoutY="77.0" promptText="CNPJ:" style="-fx-border-color: rgb(186, 228, 179); -fx-border-radius: 5;" />
      <Button layoutX="115.0" layoutY="211.0" mnemonicParsing="false" onAction="#completeSignup" prefHeight="26.0" prefWidth="162.0" style="-fx-background-color: #ffff; -fx-border-color: rgb(186, 228, 179); -fx-border-radius: 5;" text="Sign up" />
      <TextField fx:id="passwordField" layoutX="114.0" layoutY="121.0" promptText="Password:" style="-fx-border-color: rgb(186, 228, 179); -fx-border-radius: 5;" />
      <TextField fx:id="confirmPasswordField" layoutX="114.0" layoutY="165.0" promptText="Confirm Password" style="-fx-border-color: rgb(186, 228, 179); -fx-border-radius: 5;" />
      <Hyperlink layoutX="118.0" layoutY="249.0" onAction="#completeSignup" text="Already has an account?" />
   </children>
</AnchorPane>
